<h1>Welcome!</h1>
<p><%#= link_to 'Authorize via Auth2', url %></p>

<% if params[:access_token] %>
<%#=session[:access_token]%>
  <p><%= link_to 'Get User', "http://localhost:3000/api/user?access_token=#{params[:access_token]}" %></p>
  <p><%= link_to 'Update User', "http://localhost:3000/api/user/update?access_token=#{params[:access_token]}" %></p>
<% else %>
  <p><%= link_to 'Authorize', new_oauth_token_path(params) %></p>
<% end %>